<div class="tags-container">
    {{#get "tags" limit="all"}}
        {{#foreach tags}}
        <div class="FCJ-tag-card">
            <a class="FCJ-tag-card-link" href="{{url}}">
                <div class="FCJ-tag-card-image">
                    <img src="{{feature_image}}" alt="{{name}}">
                    <div class="FCJ-tag-card-title">{{name}}</div>
                </div>
            </a>
        </div>
        {{/foreach}}
    {{/get}}
</div>
